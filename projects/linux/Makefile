# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release)
  gmsv_antifreeze_config = release

else ifeq ($(config),release64)
  gmsv_antifreeze_config = release64

else ifeq ($(config),debug)
  gmsv_antifreeze_config = debug

else ifeq ($(config),debug64)
  gmsv_antifreeze_config = debug64

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := gmsv_antifreeze

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

gmsv_antifreeze:
ifneq (,$(gmsv_antifreeze_config))
	@echo "==== Building gmsv_antifreeze ($(gmsv_antifreeze_config)) ===="
	@${MAKE} --no-print-directory -C . -f gmsv_antifreeze.make config=$(gmsv_antifreeze_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f gmsv_antifreeze.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release"
	@echo "  release64"
	@echo "  debug"
	@echo "  debug64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   gmsv_antifreeze"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"